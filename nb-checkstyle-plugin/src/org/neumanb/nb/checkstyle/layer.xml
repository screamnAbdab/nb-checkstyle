<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="org-neumanb-nb-checkstyle-options-CheckstyleOptions.instance">
                <attr name="instanceClass" stringvalue="org.neumanb.nb.checkstyle.options.CheckstyleOptions"/>
            </file>
        </folder>
    </folder>
    
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="org-neumanb-nb-checkstyle-resources-checkstyle-warn-annotation.xml"
                  url="nbresloc:/org/neumanb/nb/checkstyle/resources/warn-annotation.xml"/>
            <file name="org-neumanb-nb-checkstyle-resources-checkstyle-severe-annotation.xml"
                  url="nbresloc:/org/neumanb/nb/checkstyle/resources/severe-annotation.xml"/>
        </folder>
    </folder>
    
    <folder name="TaskList">
        <folder name="Groups">
            <file name="org-neumanb-nb-checkstyle-tasklist-CheckstyleTaskGroup.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>
                
                <attr name="localizingBundle" stringvalue="org.neumanb.nb.checkstyle.tasklist.Bundle"/>
                <attr name="groupName" stringvalue="org-neumanb-nb-checkstyle-Task"/>
                <attr name="diplayNameKey" stringvalue="CheckstyleTaskGroup.label"/>
                <attr name="descriptionKey" stringvalue="CheckstyleTaskGroup.hint"/>
                <attr name="iconKey" stringvalue="CheckstyleTaskGroup.icon"/>
                <attr name="position" intvalue="250"/>
            </file>            
        </folder>
        
        <folder name="Scanners">
            <file name="org-neumanb-nb-checkstyle-tasklist-CheckstyleTaskScanner.instance">
                <attr name="instanceClass" stringvalue="org.neumanb.nb.checkstyle.tasklist.CheckstyleTaskScanner"/>
            </file>         
        </folder>
    </folder> 
    
</filesystem>
